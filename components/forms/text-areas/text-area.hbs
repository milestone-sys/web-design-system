                    <label class="msds-form__input-container-text-area-label"
                        for="default-input">{{placeholder}}</label>
                    <p class="msds-form__input-container-text-area-message">{{message}} <span class="word-count"></span>
                    </p>
                </div>
            </div>

            <div class="col-5">
