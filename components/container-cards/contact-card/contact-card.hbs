              <svg title="linkedin" class="msds-container-contact-card__icon">
                <use href="{{ path '/msds-spritemap.svg' }}#linkedin" />
              </svg>

              {{contactSocialMediaLinkNoImage}}
            </a>
          </div>

        </div>
      </div>
    </div>

    <div class="col-6">
      <div
        class="msds-container-contact-card msds-container-contact-card--small-without-image">

        <div class="msds-container-contact-card__overlay">
        </div>

        <div class="msds-container-contact-card__headline">
          {{headlineNoImage}}
        </div>

        <div class="msds-container-contact-card__body">

          <div class="msds-container-contact-card__body-contact-person">
            {{contactPersonNoImage}}
          </div>

          <div class="msds-container-contact-card__body-contact-position">
            {{contactPositionNoImage}}
          </div>

          <div class="msds-container-contact-card__body-contact-email">
            <a class="msds-container-contact-card__link" href="#">
              <svg title="email" class="msds-container-contact-card__icon">
                <use href="{{ path '/msds-spritemap.svg' }}#email" />
              </svg>
              {{contactEmailNoImage}}
            </a>
          </div>

          <div class="msds-container-contact-card__body-contact-social-media">
            <a class="msds-container-contact-card__link" href="#">

              <svg title="linkedin" class="msds-container-contact-card__icon">
                <use href="{{ path '/msds-spritemap.svg' }}#linkedin" />
              </svg>

              {{contactSocialMediaLinkNoImage}}
            </a>
          </div>
      </div>
      </div>
    </div>
  </div>
</div>